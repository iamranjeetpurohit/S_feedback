<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.sfeedback">



    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:roundIcon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity
            android:name=".SplashActivity"
            android:label="S Feedback">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>


        <activity
            android:name=".AdminLogin"
            android:label="@string/title_activity_admin_login"/>
        <activity
            android:name=".AdminHome"
            android:label="@string/title_activity_admin_home"/>
        <activity
            android:name=".FacultyList"
            android:label="@string/title_activity_faculty_list"/>
        <activity
            android:name=".AddFaculty"
            android:label= "@string/title_activity_add_faculty"/>
        <activity
            android:name=".FeedbackHome"
            android:label="Choose Faculty for Feedback" />
        <activity
            android:name=".UploadFeedback"
            android:label="@string/title_activity_upload_feedback"/>
        <activity
            android:name=".ViewFeedback"
            android:label="@string/title_activity_view_feedback"/>
        <activity
            android:name=".FacultyScreen"
            android:label="@string/title_activity_faculty_screen"/>
        <activity android:name=".StudentLoginActivity"
            android:label="Student Login"/>
        <activity android:name=".StudentSignupActivity"
            android:label="Student SignUp"/>
    </application>

</manifest>